{{#if @model}}
  {{#if @queryParams}}
    <MdcIconButtonLinkTo
        class="mdc-top-app-bar__navigation-icon"
        @route={{@route}}
        @model={{@model}}
        @queryParams={{@queryParams}}
        @icon={{this.icon}}
        @label={{this.label}}
        ...attributes />
  {{else}}
    <MdcIconButtonLinkTo
        class="mdc-top-app-bar__navigation-icon"
        @route={{@route}}
        @model={{@model}}
        @icon={{this.icon}}
        @label={{this.label}}
        ...attributes />
  {{/if}}
{{else}}
  <MdcIconButtonLinkTo
      class="mdc-top-app-bar__navigation-icon"
      @route={{@route}}
      @icon={{this.icon}}
      @label={{this.label}}
      ...attributes />
{{/if}}
