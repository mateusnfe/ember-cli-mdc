<ul class="mdc-list
  {{if @twoLine "mdc-list--two-line"}}
  {{this.leadingTileStyle}}
  {{if @nonInteractive "mdc-list--non-interactive"}}
  {{if @dense "mdc-list--dense"}}"
    ...attributes
    {{did-insert this.didInsert}}
    {{did-update this.vertical @vertical}}
    {{did-update this.singleSelection @singleSelection}}
    {{did-update this.wrapFocus @wrapFocus}}>
  {{yield}}
</ul>
