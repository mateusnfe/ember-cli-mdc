<div class="mdc-chip-set {{this._type}} {{this.label}}" ...attributes role="grid" {{did-insert this.didInsert}} {{did-update this.sync this.syncKey}} >
  {{#if (has-block)}}
    {{yield}}
  {{else}}
    {{#each @chips as |chip index|}}
      <MdcChip @id={{get chip this.idKey}}
               @text={{get chip this.textKey}}
               @leadingIcon={{get chip this.leadingIconKey}}
               @trailingIcon={{get chip this.trailingIconKey}}
               @tabindex={{index}}
               @remove={{this.removeChip}} />
    {{/each}}
  {{/if}}
</div>
